<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    In the example below, the "SetAttributes" transform will change the value of
    "connectionString" to use "ReleaseSQLServer" only when the "Match" locator
    finds an attribute "name" that has a value of "MyDB".

    <connectionStrings>
      <add name="MyDB"
        connectionString="Data Source=ReleaseSQLServer;Initial Catalog=MyReleaseDB;Integrated Security=True"
        xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
  -->
  <connectionStrings>
    <add name="NameKContext" connectionString="Data Source=192.168.1.180;Initial Catalog=Namek;Integrated Security=False;Persist Security Info=False;User ID=Namek;Password=Namek@2017;MultipleActiveResultSets=true;Connection Timeout=30; Min Pool Size=1; Max Pool Size=40;Pooling=true;" providerName="System.Data.SqlClient" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="NameKContextHangfire" connectionString="Data Source=192.168.1.180;Initial Catalog=Namek-hangfire;Integrated Security=False;Persist Security Info=False;User ID=Namek;Password=Namek@2017;MultipleActiveResultSets=true;Connection Timeout=30; Min Pool Size=1; Max Pool Size=40;Pooling=true;" providerName="System.Data.SqlClient" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <add name="NameKContext_VmPerformance" connectionString="Data Source=192.168.1.180;Initial Catalog=Namek-vmHistories;Integrated Security=False;Persist Security Info=False;User ID=Namek;Password=Namek@2017;MultipleActiveResultSets=true;Connection Timeout=5; Min Pool Size=1; Max Pool Size=40;Pooling=true;" providerName="System.Data.SqlClient" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
  </connectionStrings>
  <appSettings>
    <add key="ApiGateWay" value="http://api.Namek.vnit.top" xdt:Transform="SetAttributes"
         xdt:Locator="Match(key)" />
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <!--
      In the example below, the "Replace" transform will replace the entire
      <customErrors> section of your web.config file.
      Note that because there is only one customErrors section under the
      <system.web> node, there is no need to use the "xdt:Locator" attribute.

      <customErrors defaultRedirect="GenericError.htm"
        mode="RemoteOnly" xdt:Transform="Replace">
        <error statusCode="500" redirect="InternalError.htm"/>
      </customErrors>
    -->
  </system.web>
</configuration>